            }
            <a class="commona" style="height:35px;">|</a>
            <a href="/clt_Login_register.html" rel="nofollow">帮助中心</a>
            <a href="/about.html" style="margin-right:48px;" rel="nofollow">关于我们</a>
        </div>
    </div>
    <div class="hwrap">
        <div class="navwrap" >
            <a href="/" class="logo"></a>
            <!-- <div class="navs"> -->
            <a href="/">首页</a>
            <a href="/investlist.html">我要投资</a>
            <a href="/Borrow.html">我要借款</a>

            <a href="/securityIndex.html" name="Ms_">安全保障</a>

            <a href="/guide.html" name="Ms_">新手指引</a>

            <a href="/usercenter/index" rel="nofollow">我的账户</a>
            @*<img src="/images/toptel.png" alt="" class="phimg">*@
            <!-- </div> -->
        </div>
    </div>
    <!--内容区域-->
