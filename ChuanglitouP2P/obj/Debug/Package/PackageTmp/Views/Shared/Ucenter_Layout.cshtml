            {
                <a class="hea_em4 hea_a" href="/usercenter/index" rel="nofollow">欢迎您，@Utils.GetUsrNameSex() </a><a class="hea_a hea_altho" href="/logout.html" rel="nofollow">[退出]</a>
            }
            else
            {
