		        slidesPerView: 1,
		        paginationClickable: true,
		        spaceBetween: 30,
		        loop: true,
		        autoplay:2000
		    });

		    $(function(){
				$(window).scroll(function() {
					if($(window).scrollTop() >= 100){
						$('.actGotop').fadeIn(300);
					}else{
						$('.actGotop').fadeOut(300);
					}
				});
				$('.actGotop').click(function(){
				$('html,body').animate({scrollTop: '0px'}, 800);});
			});


